{
  "manifest_version": 2,
  "name": "UFGD - SIGECAD Boost",
  "version": "3.0.0",

  "description": "Plugin insere um botão chamado 'Comparar matérias' para poder comparar as matérias concluídas com as que faltam fazer.",

  "icons": {
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "64": "icons/icon-64.png",
    "128": "icons/icon-128.png"
  },

  "content_scripts": [
    {
      "matches": ["*://sigecad-academico.ufgd.edu.br/consultar/estrutura_curricular.*"],
      "js": [
        "jquery-3.1.1.slim.min.js",
        "variaveisDeEstado.js",
        "extrairInfo.js", "pegarConteudoMaterias.js",
        "mostrarMateriasConcluidas.js","materiaExiste.js", "selecionarMateriasEquivalentes.js",
        "botoes.js", "carregarMateriasEquivalentes.js"]
    }
  ],

  "permissions" : ["storage"]

}
